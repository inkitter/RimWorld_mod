<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_GodBelt</defName>
    <label>GodBelt</label>
    <description>A simple set of belt.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/ShieldBelt/ShieldBelt</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>10</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <generateCommonality>0</generateCommonality>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <tradeability>Sellable</tradeability>
    <statBases>
      <MarketValue>0</MarketValue>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToMake>1600</WorkToMake>
      <Mass>0.5</Mass>
      <StuffEffectMultiplierArmor>10</StuffEffectMultiplierArmor>
      <ArmorRating_Sharp>10</ArmorRating_Sharp>
      <ArmorRating_Blunt>10</ArmorRating_Blunt>
      <ArmorRating_Heat>10</ArmorRating_Heat>
      <StuffEffectMultiplierInsulation_Cold>10</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>10</StuffEffectMultiplierInsulation_Heat>
      <!-- <EnergyShieldEnergyMax>10</EnergyShieldEnergyMax>
      <EnergyShieldRechargeRate>0.5</EnergyShieldRechargeRate> -->
      <EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
			<SocialImpact>0.15</SocialImpact>
      <CarryBulk>5</CarryBulk>
      <ReloadSpeed>0.1</ReloadSpeed>
		</equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
        <li>Torso</li>
        <li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
        <li>Waist</li>
        <li>Eyes</li>
        <li>Teeth</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
        <li>Belt</li>
      </layers>
      <tags>
        <li>IndustrialBasic</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>


  <ThingDef ParentName="ApparelCarryGearBase">
    <defName>Apparel_GodBackpack</defName>
    <label>God backpack</label>
    <description>God big bag.</description>
    <techLevel>Neolithic</techLevel>
    <graphicData>
      <texPath>Things/Apparel/TribalPack/Tribalpack</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>Sellable</tradeability>
    <generateCommonality>0</generateCommonality>
    <statBases>
      <MaxHitPoints>1000</MaxHitPoints>
      <WorkToMake>5000</WorkToMake>
      <Mass>1</Mass>
      <Bulk>1</Bulk>
      <StuffEffectMultiplierArmor>10</StuffEffectMultiplierArmor>
      <ArmorRating_Sharp>10</ArmorRating_Sharp>
      <ArmorRating_Blunt>10</ArmorRating_Blunt>
      <ArmorRating_Heat>10</ArmorRating_Heat>
      <StuffEffectMultiplierInsulation_Cold>10</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>10</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <CarryBulk>15</CarryBulk>
      <ReloadSpeed>0.1</ReloadSpeed>
    </equippedStatOffsets>
    <costStuffCount>10</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <recipeMaker>
      <recipeUsers>
        <li>CraftingSpot</li>
        <li>HandTailoringBench</li>
        <li>ElectricTailoringBench</li>
      </recipeUsers>
    </recipeMaker>
    <apparel>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>FullHead</li>
        <li>Torso</li>
        <li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Feet</li>
        <li>Legs</li>
        <li>Waist</li>
        <li>Eyes</li>
        <li>Teeth</li>
        <li>Hands</li>
      </bodyPartGroups>
      <layers>
        <li>Backpack</li>
      </layers>
      <tags>
        <li>Neolithic</li>
      </tags>
      <wornGraphicPath>Things/Apparel/TribalPack/Tribalpack</wornGraphicPath>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
</Defs>
